		<script type="text/javascript">
			$(document).ready(function () {
				$(window).scroll(function () {
					if ($(window).scrollTop() > $(window).height() - 80) {
						$('ul#nav').addClass('Two');
					}
					else {
						$('ul#nav').removeClass('Two');
					}
				});
			});
			$(document).on('click', 'a[href^="#"]', function (e) {
				var id = $(this).attr('href');
				var $id = $(id);
				if ($id.length === 0) {
					return;
				}
				e.preventDefault();
				var pos = $id.offset().top - 49;
				$('body, html').animate({ scrollTop: pos });
			});
			$(document).ready(function () {
				$(this).parents().find('a').removeClass('active');
				$(this).addClass('active');
			});
			$(window).on('scroll', function () {
				$('section').each(function () {
					if ($(window).scrollTop() >= $(window).height() - 80) {
						if ($(window).scrollTop() >= $(this).position().top - 80) {
							var id = $(this).attr('id');
							$('ul#nav li a').removeClass('active');
							$('ul#nav li a[href="#' + id + '"]').addClass('active');
							$('ul#nav').attr('class', ('Two' + ' ' + id));
						}
					}
					else {

						$('ul#nav').removeClass('Two');
					}
				});
			});

			function myFunction() {
				var x = document.getElementById("mobileNav");
				$(".icon i").toggleClass('fa-bars fa-times');
				if (x.className === "menu") {
					x.className += " responsive";
					$(".menu").slideDown("slow", function () { });
				} else {
					x.className = "menu";
					$(".menu").slideUp("slow", function () { });
				}
			}
		</script>
		<script type="text/javascript">
			$(document).ready(function () {
				$('#container').imagesLoaded({ background: true }, function () {
					$('#loader-wrapper').fadeOut(250);
					$('.pageLoader').css('opacity', '1');
					$('.pageLoader2').css("transform", "translateY(0)");
					$('.pageSection').css('opacity', '1');
				});
			});
		</script>
